<VirtualHost *:8080>
        ServerAdmin $$$$EMAIL_ADDRESS$$$$
        ServerName $$$$PROD_APP_HOST$$$$
        ServerAlias media.$$$$PROD_APP_HOST$$$$
        DocumentRoot /home/$$$$PROJECT_NAME$$$$/$$$$PROJECT_NAME$$$$/media

        WSGIDaemonProcess $$$$PROJECT_NAME$$$$-prod user=$$$$PROJECT_NAME$$$$ group=$$$$PROJECT_NAME$$$$ processes=5 threads=1 maximum-requests=10000 python-path=/home/$$$$PROJECT_NAME$$$$/virtualenvs/$$$$PROJECT_NAME$$$$/lib/python2.5/site-packages
        WSGIProcessGroup $$$$PROJECT_NAME$$$$-prod
        WSGIScriptAlias / /home/$$$$PROJECT_NAME$$$$/$$$$PROJECT_NAME$$$$/conf/$$$$PROJECT_NAME$$$$-prod.wsgi
</VirtualHost>