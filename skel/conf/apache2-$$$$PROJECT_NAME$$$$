<VirtualHost *:8080>
        ServerAdmin $$$$EMAIL_ADDRESS$$$$
        ServerName $$$$DEV_APP_HOST$$$$
        ServerAlias media-$$$$DEV_APP_HOST$$$$
        DocumentRoot /home/$$$$PROJECT_NAME$$$$/$$$$PROJECT_NAME$$$$/media

        WSGIDaemonProcess $$$$PROJECT_NAME$$$$-dev user=$$$$PROJECT_NAME$$$$ group=$$$$PROJECT_NAME$$$$ processes=3 threads=1 maximum-requests=1000 python-path=/home/pictionnected/virtualenvs/$$$$PROJECT_NAME$$$$/lib/python2.5/site-packages
        WSGIProcessGroup $$$$PROJECT_NAME$$$$-dev
        WSGIScriptAlias / /home/$$$$PROJECT_NAME$$$$/$$$$PROJECT_NAME$$$$/conf/$$$$PROJECT_NAME$$$$-dev.wsgi
</VirtualHost>